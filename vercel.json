{
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm ci --prefer-offline --cache /tmp/.npm",
  "framework": "nextjs",
  "regions": ["iad1"],
  "functions": {
    "app/api/newsletter/route.ts": {
      "maxDuration": 10
    },
    "app/api/og/route.tsx": {
      "maxDuration": 10
    }
  },
  "headers": [
    {
      "source": "/(.*)\\.(js|css|map)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, immutable, max-age=31536000"
        }
      ]
    },
    {
      "source": "/(.*)\\.(jpg|jpeg|gif|png|svg|ico|webp|avif)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, must-revalidate"
        }
      ]
    },
    {
      "source": "/fonts/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, immutable, max-age=31536000"
        }
      ]
    }
  ],
  "rewrites": [],
  "github": {
    "silent": true
  },
  "crons": []
}
